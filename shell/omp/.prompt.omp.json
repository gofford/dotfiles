{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#61afef",
          "properties": {
            "style": "agnoster"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "type": "git",
          "style": "plain",
          "foreground": "#abb2bf",
          "properties": {
            "branch_icon": " ",
            "fetch_status": true
          },
          "template": "{{ .HEAD }}{{ if gt .Ahead 0 }}<#56b6c2> ↑{{ .Ahead }}</>{{ end }}{{ if gt .Behind 0 }}<#56b6c2> ↓{{ .Behind }}</>{{ end }}{{ if or .Working.Changed .Staging.Changed (gt .StashCount 0) }} |{{ end }}{{ if .Working.Changed }} <#e5c07b> {{ .Working.String }}</>{{ end }}{{ if .Staging.Changed }} <#98c379> {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} <#abb2bf>  {{ .StashCount }}</>{{ end }}"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "python",
          "style": "diamond",
          "leading_diamond": " ",
          "trailing_diamond": "",
          "foreground": "#282c34",
          "background": "#98c379",
          "template": "  {{ .Venv }} "
        },
        {
          "type": "terraform",
          "style": "diamond",
          "leading_diamond": " ",
          "trailing_diamond": "",
          "foreground": "#282c34",
          "background": "#e5c07b",
          "template": "{{if contains \"default\" .WorkspaceName}}{{\"\"}}{{else}} 󰏗 {{.WorkspaceName}} {{end}}"
        },
        {
          "type": "gcp",
          "style": "diamond",
          "leading_diamond": " ",
          "trailing_diamond": "",
          "foreground": "#282c34",
          "background": "#56b6c2",
          "template": "  {{ .ActiveConfig }} "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#c678dd",
          "foreground_templates": [
            "{{ if gt .code 0 }}#e06c75{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u276f ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "type": "rprompt",
      "segments": [
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#5c6370",
          "properties": {
            "threshold": 2000,
            "style": "round"
          },
          "template": "took {{ .FormattedMs }} "
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "#5c6370",
          "template": "at {{ .CurrentDate | date \"15:04\" }}"
        }
      ]
    }
  ],
  "console_title_template": "{{if .Root}}(Admin){{end}} {{.PWD}}",
  "transient_prompt": {
    "foreground": "#c678dd",
    "foreground_templates": [
      "{{ if gt .Code 0 }}#e06c75{{ end }}"
    ],
    "template": "\u276f "
  },
  "version": 3
}
